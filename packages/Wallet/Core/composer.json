{
  "name": "wallet/core",
  "description": "Wallet is a minimal, composable payment core for Laravel.",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "wallet", "core"],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "rappasoft/laravel-livewire-tables": "^3.7",
    "africastalking/africastalking": "^3.0",
    "akika/laravel-mpesa-multivendor": "^0.2.0",
    "laravel/horizon": "^5.22"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.7",
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Wallet\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wallet\\Core\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wallet\\Core\\Providers\\CoreServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
